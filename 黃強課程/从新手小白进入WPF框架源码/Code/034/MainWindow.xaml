<Window x:Class="_034.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_034"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <WrapPanel Orientation="Horizontal" ItemHeight="100" ItemWidth="200">
        <Button x:Name="button" Content="Button"/>
        <Button x:Name="button1" Content="Button"/>
        <Button x:Name="button2" Content="Button"/>
        <Button x:Name="button3" Content="Button"/>
        <Button x:Name="button4" Content="Button"/>

    </WrapPanel> 
</Window>
