<Window x:Class="_038.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_038"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Border Background="#FF89FC0D"  Width="200"  Height="200" BorderBrush="#FFF70909" BorderThickness="10,10,10,10" CornerRadius="100,100,100,100">

            <Image x:Name="image"   Source="/images/ball.png" Stretch="Fill">
                <Image.Clip> 
                    <EllipseGeometry  Center="90,90"   RadiusX="90"  RadiusY="90" ></EllipseGeometry>
                </Image.Clip>
            </Image>


        </Border>
   
    </Grid>
</Window>
